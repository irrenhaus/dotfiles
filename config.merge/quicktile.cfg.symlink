[general]
cfg_schema = 1
ColumnCount = 3
UseWorkarea = True
ModMask = <Ctrl><Alt>
MovementsWrap = True

[keys]
C = move-to-center
H = horizontal-maximize
V = vertical-maximize
BackSpace = monitor-switch
Delete = bottom-left
End = bottom
Next = bottom-right
Insert = left
Home = middle
Prior = right
Print = top-left
ScrollLock = top
Pause = top-right
Return = maximize
Left = monitor-prev
Right = monitor-next
Up = top
Down = bottom
